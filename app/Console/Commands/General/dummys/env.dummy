APP_NAME=Microauth
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=UTC

LOG_CHANNEL=stack
LOG_SLACK_WEBHOOK_URL=

DB_CONNECTION=mysql
DB_HOST={var_db_host}
DB_PORT={var_db_port}
DB_DATABASE={var_db_database}
DB_USERNAME={var_db_username}
DB_PASSWORD={var_db_password}

CACHE_DRIVER=file
QUEUE_CONNECTION=sync

AUTH_SIGNATURE={var_auth_signature}
AUTH_TOKEN_EXPIRATION_TIME={var_auth_expiration_time}
